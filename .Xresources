URxvt.font: xft:Terminus:12

URxvt.geometry: 80x50
URxvt.saveLines: 10000
URxvt.cutchars: "()*,<>[]{}|''"
URxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/+$2 \\Q$1\\E/
URxvt.skipBuiltinGlyphs: true
URxvt.url-launcher: /usr/bin/firefox
URxvt*colorUL: orange

URxvt.scrollBar: false
URxvt.visualBell: true
! URxvt spells "eightBitInput" as "meta8" and defaults it to false
! URxvt.depth: 32
URxvt.internalBorder: 10

! Layout
rofi.width:                          50
rofi.lines:                          15
rofi.columns:                        1
rofi.color-enabled:                  true
rofi.color-normal:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-urgent:                   #282828,#d79921,#353535,#d79921,#282828
rofi.color-active:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-window:                   #282828,#458488

! Behaviour
rofi.terminal:                       urxvt
rofi.levenshtein-sort:               true
rofi.fuzzy:                          true
rofi.kb-cancel:                      Escape,Control+c

! Font settings
#Xft.dpi:                    210
Xft.autohint:               0
Xft.hintstyle:              hintfull
Xft.hinting:                true
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.lcdfilter:              lcddefault
#Xft.dpi:                    144
